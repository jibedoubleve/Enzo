<UserControl
    x:Class="Enzo.Controls.TestCharlieResult"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Enzo.Controls"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <StackPanel VerticalAlignment="Center">
        <Image Height="{StaticResource DefaultPhotoHeight}" Source="/photo/places/place_lego.jpeg" />
        <Button
            Margin="30"
            Click="OnGo"
            Style="{StaticResource ValidationButtonStyle}" />
    </StackPanel>
</UserControl>
